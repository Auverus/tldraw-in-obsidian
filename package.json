{
	"name": "tldraw-in-obsidian",
	"version": "0.15.0",
	"description": "This plugin integrates Tldraw into Obsidian, allowing users to edit Tldraw drawings and saving the data as a plain markdown file.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"dist": "node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"tldraw",
		"drawing",
		"whiteboard",
		"notes"
	],
	"author": "Sam Alhaqab",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"esbuild-plugin-svgr": "^2.0.0",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@tldraw/tldraw": "^2.0.0-alpha.14",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"use-debounce": "^9.0.4",
		"zustand": "^4.3.9"
	}
}
